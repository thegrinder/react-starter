{
  "name": "react-starter",
  "version": "0.0.0",
  "description": "React starter",
  "keywords": [
    "basic-styled-uikit",
    "react",
    "styled-components",
    "tailwindcss"
  ],
  "homepage": "https://github.com/thegrinder/react-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/react-starter.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint './{app,config}/**/*.js' --fix --config config/eslint/eslint.config.js",
    "production": "webpack --config config/webpack/webpack.prod.js",
    "prettier": "prettier './{app,config}/**/*.js' --write --config config/prettier/prettier.config.js",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.js",
    "test": "jest --config config/jest/jest.config.js"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config config/commitlint/commitlint.config.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-prefix-selector": "^1.7.2",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.5.1",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "basic-styled-uikit": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-handy-hooks": "^1.9.0",
    "react-intl": "^5.3.2",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  }
}
