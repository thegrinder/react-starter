{
  "name": "rr-starter",
  "version": "0.0.0",
  "description": "React redux starter",
  "keywords": [
    "react",
    "redux",
    "styled-components",
    "redux-saga",
    "basic-styled-uikit",
    "tailwindcss"
  ],
  "homepage": "https://github.com/thegrinder/rr-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/rr-starter.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "production": "webpack --config config/webpack.prod.js",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^app(.*)$": "<rootDir>/app/$1"
    }
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "commitizen": "^3.0.7",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "dom-testing-library": "^3.16.8",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "lint-staged": "^8.1.5",
    "postcss-loader": "^3.0.0",
    "react-testing-library": "^6.0.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "basic-styled-uikit": "^1.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-intl": "^2.8.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.1.3",
    "tailwindcss": "^0.7.4",
    "tailwindcss-base-plugins": "^1.0.4",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0"
  }
}
